<svg width="180" height="180" viewBox="0 0 180 180" fill="none" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="appleGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#667eea;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#764ba2;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="sparkleGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ffd700;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ffed4e;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background with rounded corners for iOS -->
  <rect width="180" height="180" rx="32" fill="url(#appleGradient)"/>
  
  <!-- Main cleaning spray icon -->
  <g transform="translate(45, 45)">
    <!-- Spray bottle -->
    <rect x="35" y="15" width="15" height="8" rx="2" fill="white" opacity="0.95"/>
    <rect x="40" y="8" width="5" height="10" fill="white"/>
    <path d="M35 23h15v45c0 4-3.5 7-7.5 7s-7.5-3-7.5-7V23z" fill="white" opacity="0.9"/>
    
    <!-- Sparkles around the bottle -->
    <g fill="url(#sparkleGradient)">
      <!-- Large sparkles -->
      <circle cx="65" cy="25" r="6"/>
      <circle cx="75" cy="45" r="5"/>
      <circle cx="8" cy="35" r="5"/>
      <circle cx="15" cy="55" r="6"/>
      <circle cx="70" cy="65" r="5"/>
      <circle cx="5" cy="65" r="5"/>
      <circle cx="20" cy="15" r="4"/>
      <circle cx="60" cy="15" r="4"/>
      
      <!-- Medium sparkles -->
      <circle cx="25" cy="25" r="3"/>
      <circle cx="55" cy="35" r="3"/>
      <circle cx="80" cy="55" r="3"/>
      <circle cx="10" cy="75" r="3"/>
      <circle cx="85" cy="30" r="3"/>
      
      <!-- Small sparkles -->
      <circle cx="30" cy="45" r="2"/>
      <circle cx="50" cy="75" r="2"/>
      <circle cx="15" cy="25" r="2"/>
      <circle cx="75" cy="25" r="2"/>
      <circle cx="85" cy="70" r="2"/>
      <circle cx="25" cy="75" r="2"/>
    </g>
    
    <!-- Star-shaped sparkles for extra effect -->
    <g fill="url(#sparkleGradient)">
      <g transform="translate(20, 40) scale(1.5)">
        <path d="M0,-3 L1,0 L3,0 L1,1.5 L2,4 L0,2.5 L-2,4 L-1,1.5 L-3,0 L-1,0 Z"/>
      </g>
      <g transform="translate(65, 50) scale(1.2)">
        <path d="M0,-3 L1,0 L3,0 L1,1.5 L2,4 L0,2.5 L-2,4 L-1,1.5 L-3,0 L-1,0 Z"/>
      </g>
      <g transform="translate(80, 20) scale(1)">
        <path d="M0,-3 L1,0 L3,0 L1,1.5 L2,4 L0,2.5 L-2,4 L-1,1.5 L-3,0 L-1,0 Z"/>
      </g>
    </g>
  </g>
</svg>
